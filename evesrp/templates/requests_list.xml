{% extends "base.xml" %}
{% from "macros.xml" import request_xml %}

{% block content %}
<requests>
  <total-payouts>{{ total_payouts.currency() }}</total-payouts>
  {% for srp_request in requests %}
    {{ request_xml(srp_request) }}
  {% endfor %}
</requests>
{% if prev is defined %}
<prev>{{ prev }}</prev>
{% endif %}
{% if next is defined %}
<next>{{ next }}</next>
{% endif %}
{% endblock content %}
